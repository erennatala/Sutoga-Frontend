    "date-fns": "^2.30.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "electron-main-fetch": "^1.0.0",
    "electron-store": "^8.1.0",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-webpack-plugin": "^3.1.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "history": "^5.3.0",
