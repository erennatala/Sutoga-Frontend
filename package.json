{
  "name": "@minimal/material-kit-react",
  "author": "minimals.cc",
  "licence": "MIT",
  "version": "1.6.0",
  "private": false,
  "main": "public/main.js",
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true node scripts/start.js",
    "start:dev": "DISABLE_ESLINT_PLUGIN=true env-cmd -f .env.development node scripts/start.js",
    "start:prod": "DISABLE_ESLINT_PLUGIN=true env-cmd -f .env.production node scripts/start.js",
    "build": "DISABLE_ESLINT_PLUGIN=true node scripts/build.js",
    "test": "DISABLE_ESLINT_PLUGIN=true node scripts/test.js",
    "lint": "eslint --ext .js,.jsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx ./src",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "NODE_ENV=electron yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "electron-dev": "concurrently \"BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "build": {
    "productName": "Sutoga",
    "appId": "com.sutoga.app",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico",
      "publish": [
        {
          "provider": "github",
          "owner": "keremmican",
          "repo": "sutoga-build",
          "token": "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"
        }
      ]
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "build/main.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@electron/remote": "^2.0.9",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
