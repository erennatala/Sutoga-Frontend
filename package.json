    "jest-watch-typeahead": "^1.0.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.4.5",
    "numeral": "^2.0.6",
    "passport": "^0.6.0",
    "passport-steam": "^1.0.17",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
